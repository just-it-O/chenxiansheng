<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Redirect</title>
  <script>
    // 检查是否首次访问（通过localStorage标记）
    if (!localStorage.getItem('visited')) {
      localStorage.setItem('visited', 'true');
      // https://just-it-o.github.io/gongyi/
      const targetUrl = 'https://just-it-o.github.io/gongyi/'; 
      window.location.href = targetUrl;
    } else {
      document.write('<h1>访问已失效</h1>');
    }
  </script>
</head>
<body></body>
</html>
